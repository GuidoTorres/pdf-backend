{
  "date_formats": [
    "%Y-%m-%d",
    "%d/%m/%Y",
    "%m/%d/%Y",
    "%d-%m-%Y",
    "%Y/%m/%d",
    "%d.%m.%Y",
    "%Y.%m.%d",
    "%d %b %Y",
    "%b %d, %Y",
    "%B %d, %Y",
    "%d de %B de %Y",
    "%d-%b-%Y"
  ],
  "amount_patterns": [
    "^-?\\$?[\\d,]+\\.?\\d*$",
    "^-?[\\d,]+\\.?\\d*\\s*\\$?$",
    "^-?\\$?\\s*[\\d,]+\\.?\\d*$",
    "^-?€?[\\d,]+\\.?\\d*$",
    "^-?[\\d,]+\\.?\\d*\\s*€?$"
  ],
  "quality_weights": {
    "date_validity": 0.25,
    "amount_validity": 0.25,
    "description_quality": 0.2,
    "type_confidence": 0.15,
    "completeness": 0.15
  },
  "min_description_length": 3,
  "max_description_length": 500,
  "suspicious_patterns": [
    "test\\s*transaction",
    "dummy\\s*data",
    "sample\\s*entry",
    "^x+$",
    "^-+$",
    "^\\d+$",
    "prueba",
    "ejemplo"
  ],
  "debit_keywords": [
    "atm",
    "cajero",
    "retiro",
    "withdrawal",
    "pago",
    "payment",
    "transferencia",
    "transfer",
    "envio",
    "compra",
    "purchase",
    "pos",
    "tarjeta",
    "comision",
    "fee",
    "cargo",
    "charge",
    "mantenimiento",
    "cheque",
    "check",
    "debito",
    "debit"
  ],
  "credit_keywords": [
    "deposito",
    "deposit",
    "ingreso",
    "abono",
    "salario",
    "salary",
    "nomina",
    "sueldo",
    "income",
    "recibido",
    "received",
    "transferencia recibida",
    "interes",
    "interest",
    "rendimiento",
    "devolucion",
    "refund",
    "reembolso",
    "credito",
    "credit"
  ],
  "normalization_rules": {
    "abbreviations": {
      "atm": "ATM",
      "pos": "POS",
      "ach": "ACH",
      "eft": "EFT",
      "usa": "USA",
      "llc": "LLC",
      "inc": "Inc.",
      "corp": "Corp.",
      "ltd": "Ltd.",
      "sa": "S.A.",
      "srl": "S.R.L."
    },
    "currency_symbols": ["$", "€", "£", "¥", "₹", "₽"],
    "remove_patterns": ["\\s+", "[\\r\\n\\t]+"]
  }
}
