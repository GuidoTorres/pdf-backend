{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend para el conversor de estados de cuenta PDF",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "postinstall": "pip3 install -r requirements.txt --target=./python_libs",
    "start": "node start_system.js",
    "start:production": "node scripts/production-start.js",
    "start:old": "concurrently \"node src/app.js\" \"FLASK_DEBUG=true PYTHONPATH=./python_libs python3 docling_worker.py\" \"node src/workers/pdfProcessor.js\"",
    "dev": "concurrently \"nodemon src/app.js\" \"node src/workers/pdfProcessor.js\"",
    "dev:old": "concurrently \"nodemon src/app.js\" \"FLASK_DEBUG=true PYTHONPATH=./python_libs python3 docling_worker.py\" \"node src/workers/pdfProcessor.js\"",
    "worker": "node src/workers/pdfProcessor.js",
    "test:queue": "node test_queue_system.js",
    "test:worker": "node test_worker_direct.js",
    "db:init": "node src/scripts/initDatabase.js",
    "db:reset": "node -e \"import('./src/config/database.js').then(db => db.default.sync({force: true}).then(() => process.exit(0)))\"",
    "db:cleanup": "node src/scripts/cleanupSessions.js",
    "db:test": "node test-db.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:auth": "node test/auth/run-auth-tests.js",
    "test:auth:unit": "vitest run test/auth/unit-login.test.js",
    "test:auth:traditional": "vitest run test/auth/traditional-login.test.js",
    "test:auth:google": "vitest run test/auth/google-oauth.test.js",
    "test:auth:register": "vitest run test/auth/user-registration.test.js",
    "test:auth:sessions": "vitest run test/auth/session-management.test.js",
    "test:auth:integration": "vitest run test/auth/integration-flows.test.js",
    "test:auth:analysis": "vitest run test/auth/login-analysis.test.js",
    "test:load": "node run-load-tests.js",
    "test:load:concurrent": "vitest run test/load/concurrent-processing.test.js",
    "test:load:priority": "vitest run test/load/priority-queue.test.js",
    "test:load:scaling": "vitest run test/load/auto-scaling.test.js",
    "test:load:memory": "vitest run test/load/memory-management.test.js",
    "deploy": "./scripts/deploy.sh deploy",
    "deploy:rollback": "./scripts/deploy.sh rollback",
    "deploy:status": "./scripts/deploy.sh status",
    "monitor": "./scripts/monitor.sh status",
    "monitor:continuous": "./scripts/monitor.sh monitor",
    "monitor:health": "./scripts/monitor.sh health",
    "logs:production": "docker-compose -f docker-compose.production.yml logs -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/documentai": "^9.0.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.10.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "google-auth-library": "^10.2.1",
    "groq-sdk": "^0.27.0",
    "ioredis": "^5.4.1",
    "jsonrepair": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-poppler": "^8.0.3",
    "nodemailer": "^6.9.8",
    "pdf-extract": "^1.0.11",
    "pdf-parse": "^1.1.1",
    "pdf-table-extractor": "^1.0.3",
    "pdf-text-reader": "^5.1.0",
    "sequelize": "^6.35.2",
    "socket.io": "^4.8.1",
    "tesseract.js": "^5.0.5",
    "together-ai": "^0.20.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
