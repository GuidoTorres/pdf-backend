    "@google-cloud/documentai": "^9.0.0",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@supabase/supabase-js": "^2.43.4",
    "axios": "^1.7.2",
    "bullmq": "^5.10.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "groq-sdk": "^0.27.0",
    "ioredis": "^5.4.1",
    "jsonrepair": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-poppler": "^8.0.3",
