{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend para el conversor de estados de cuenta PDF",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "postinstall": "pip3 install -r requirements.txt --target=./python_libs",
    "start": "concurrently \"node src/app.js\" \"FLASK_DEBUG=true PYTHONPATH=./python_libs python3 docling_worker.py\" \"node src/workers/pdfProcessor.js\"",
    "dev":   "concurrently \"nodemon src/app.js\" \"FLASK_DEBUG=true PYTHONPATH=./python_libs python3 docling_worker.py\" \"node src/workers/pdfProcessor.js\""
    },
